  <data>
    <row>
      <col id="0">SNES</col>
    </row>
    <row>
      <col id="0">N64</col>
    </row>
    <row>
      <col id="0">SNES</col>
    </row>
    <row>
      <col id="0">N64</col>
    </row>
  </data>
</object>
<object class="GtkListStore" id="core-store">
  <columns>
