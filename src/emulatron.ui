<object class="GtkTreeStore" id="game-store">
  <columns>
    <column type="gchararray"/>
  </columns>
</object>

<!--Change to GActions when api exists-->
<object class="GtkActionGroup" id="view-action-group">
  <child>
    <object class="GtkRadioAction" id="view-grid-action">
      <property name="label" translatable="yes">Grid View</property>
      <property name="value">1</property>
    </object>
  </child>
  <child>
    <object class="GtkRadioAction" id="view-list-action">
      <property name="label" translatable="yes">List View</property>
      <property name="group">view-grid-action</property>
