<interface>
<requires lib="gtk+" version="3.12"/>

<object class="GtkApplicationWindow" id="emu_main_window">
  <property name="title" translatable="yes">Emulatron</property>
  <property name="default-width">600</property>
  <property name="default-height">400</property>
  <property name="can_focus">False</property>
  <property name="icon">img/joy-angle-256.png</property>
  <signal name="delete-event" handler="main_window_destroy_cb" swapped="no"/>
  <child type="titlebar">
    <object class="GtkHeaderBar" id="header">
      <property name="visible">True</property>
      <property name="show-close-button">True</property>
      <child type="title">
        <object class="GtkStackSwitcher" id="emu_stack_switcher">
          <property name="visible">True</property>
          <property name="stack">emu_main_stack</property>
        </object>
      </child>
      <child>
        <object class="GtkButton" id="add_button">
          <property name="visible">True</property>
          <child internal-child="accessible">
            <object class="AtkObject" id="add-button-atkobject">
              <property name="AtkObject::accessible-name" translatable="yes">Add</property>
            </object>
          </child>
          <style>
            <class name="image-button"/>
          </style>
          <child>
            <object class="GtkImage" id="add_button_image">
              <property name="visible">True</property>
              <property name="can_focus">False</property>
              <property name="icon_name">list-add-symbolic</property>
              <property name="icon_size">1</property>
            </object>
          </child>
        </object>
      </child>
      <child>
        <object class="GtkButton" id="test_button">
          <property name="visible">True</property>
          <child internal-child="accessible">
            <object class="AtkObject" id="test-button-atkobject">
              <property name="AtkObject::accessible-name" translatable="yes">Add</property>
            </object>
          </child>
          <style>
            <class name="image-button"/>
          </style>
          <child>
            <object class="GtkImage" id="test_button_image">
              <property name="visible">True</property>
              <property name="can_focus">False</property>
              <property name="resource">/org/colinkinloch/emulatron/img/dino_down.png</property>
              <property name="icon_size">1</property>
            </object>
          </child>
        </object>
      </child>
      <child>
        <object class="GtkBox">
          <property name="visible">True</property>
          <style>
            <class name="linked"/>
          </style>
          <child>
            <object class="GtkToggleButton" id="view_list_button">
              <property name="visible">True</property>
              <property name="related_action">view-list-action</property>
              <property name="active">True</property>
              <style>
                <class name="image-button"/>
              </style>
              <child>
                <object class="GtkImage" id="view_list_button_image">
                  <property name="visible">True</property>
                  <property name="icon-name">view-list-symbolic</property>
                  <property name="icon-size">1</property>
                </object>
              </child>
            </object>
          </child>
          <child>
            <object class="GtkToggleButton" id="view_grid_button">
            <property name="visible">True</property>
            <property name="related_action">view-grid-action</property>
            <style>
              <class name="image-button"/>
            </style>
            <child>
              <object class="GtkImage" id="view_grid_button_image">
                <property name="visible">True</property>
                <property name="icon-name">view-grid-symbolic</property>
                <property name="icon-size">1</property>
              </object>
            </child>
            </object>
          </child>
          <child>
            <object class="GtkMenuButton" id="view_menu_button">
              <property name="visible">True</property>
              <property name="menu-model">view-menu</property>s
              <style>
                <class name="image-button"/>
              </style>
              <child>
                <object class="GtkImage" id="view_menu_button_image">
                  <property name="visible">True</property>
                  <property name="icon-name">go-down-symbolic</property>
                  <property name="icon-size">1</property>
                </object>
              </child>
            </object>
          </child>
        </object>
        <packing>
          <property name="pack-type">end</property>
        </packing>
      </child>
      <child>
        <object class="GtkToggleButton" id="select_button">
          <property name="visible">True</property>
          <style>
            <class name="image-button"/>
          </style>
          <child>
            <object class="GtkImage" id="select_button_image">
              <property name="visible">True</property>
              <property name="icon-name">object-select-symbolic</property>
              <property name="icon-size">1</property>
            </object>
          </child>
        </object>
        <packing>
          <property name="pack-type">end</property>
        </packing>
      </child>
      <child>
        <object class="GtkToggleButton" id="search_button">
          <property name="visible">True</property>
          <style>
            <class name="image-button"/>
          </style>
          <child>
            <object class="GtkImage" id="search_button_image">
              <property name="visible">True</property>
              <property name="icon-name">edit-find-symbolic</property>
              <property name="icon-size">1</property>
            </object>
          </child>
        </object>
        <packing>
          <property name="pack-type">end</property>
        </packing>
      </child>
    </object>
  </child>
  <child>
    <object class="GtkStack" id="emu_main_stack">
      <property name="visible">True</property>
      <property name="can_focus">False</property>
      <child>
        <object class="GtkPaned" id="emu_main_box">
          <property name="visible">True</property>
          <property name="can_focus">False</property>
          <child>
            <object class="GtkScrolledWindow">
              <property name="visible">True</property>
              <property name="can_focus">False</property>
              <child>
                <object class="GtkTreeView" id="emu_list_tree_view">
                  <property name="visible">True</property>
                  <property name="width-request">200</property>
                  <property name="model">emu-list-store</property>
                  <child>
                    <object class="GtkTreeViewColumn" id="test_column">
                      <property name="title">Consoles</property>
                      <child>
                        <object class="GtkCellRendererText" id="test-renderer"/>
                        <attributes>
                          <attribute name="text">0</attribute>
                        </attributes>
                      </child>
                    </object>
                  </child>
                  <child internal-child="selection">
                    <object class="GtkTreeSelection" id="tree_selection">
                      <!--property name="mode">browse</property>-->
                    </object>
                  </child>
                </object>
              </child>
            </object>
          </child>
          <child>
            <object class="GtkBox">
              <property name="visible">True</property>
              <child>
                <object class="GtkSearchBar" id="searchbar">
                  <property name="visible">True</property>
                  <child>
                    <object class="GtkSearchEntry" id="searchentry">
                      <property name="visible">True</property>
                      <signal name="search-changed" handler="search_text_changed"/>
                    </object>
                  </child>
                </object>
              </child>
              <child>
                <object class="GtkIconView" id="game_selector">
                  <property name="visible">True</property>
                  <property name="model">game-store</property>
                </object>
              </child>
              <!--<child>
                <object class="GtkStack" id="emuview_main_stack">
                  <property name="visible">True</property>
                  <property name="homogeneous">True</property>
                  <child>
                    <object>
                    </object>
                    <packing>
                      <property name="name">menu</property>
                    </packing>
                  </child>
                  <child>
                    <object class="GtkVBox" id="emuview_main_vbox">
                      <property name="visible">True</property>
                      <property name="can_focus">False</property>
                      <child>
                        <object class="GtkButton" id="stacktest_button">
                          <property name="visible">True</property>
                        </object>
                      </child>
                      <child>
                        <object class="GtkOverlay" id="emuview_bvw_box">
                          <property name="visible">True</property>
                          <property name="can_focus">False</property>
                        </object>
                        <packing>
                          <property name="expand">True</property>
                          <property name="fill">True</property>
                          <property name="position">0</property>
                        </packing>
                      </child>
                    </object>
                    <packing>
                      <property name="name">player</property>
                    </packing>
                  </child>
                </object>
                <packing>
                  <property name="expand">True</property>
                  <property name="fill">True</property>
                  <property name="position">0</property>
                </packing>
              </child>-->
            </object>
          </child>
        </object>
        <packing>
          <property name="name">game-selector</property>
        </packing>
      </child>
      <child>
        <object class="GtkVBox">
          <property name="visible">True</property>
          <child>
            <object class="GtkOverlay">
              <property name="visible">True</property>
              <child>
                <object class="GtkRevealer">
                  <property name="visible">True</property>
                  <child>
                    <object class="GtkHeaderBar">
                    </object>
                  </child>
                </object>
              </child>
            </object>
          </child>
        </object>
        <packing>
          <property name="name">game-view</property>
        </packing>
      </child>
    </object>
  </child>
</object>

<object class="GtkWindow" id="emu_pref_window">
  <property name="default-width">400</property>
  <property name="default-height">400</property>
  <property name="can_focus">False</property>
  <child type="titlebar">
    <object class="GtkHeaderBar" id="pref_header">
      <property name="visible">True</property>
      <property name="show-close-button">True</property>
      <child type="title">
        <object class="GtkStackSwitcher" id="pref_main_stack_switcher">
          <property name="visible">True</property>
          <property name="stack">pref_main_stack</property>
          <property name="margin">6</property>
        </object>
      </child>
    </object>
  </child>
  <child>
    <object class="GtkStack" id="pref_main_stack">
      <property name="visible">True</property>
      <child>
        <object class="GtkBox">
          <property name="visible">True</property>
          <property name="orientation">horizontal</property>
          <child>
            <object class="GtkButton">
              <property name="visible">True</property>
              <property name="label">Hi</property>
            </object>
          </child>
        </object>
        <packing>
          <property name="icon-name">accessories-dictionary-symbolic</property>
          <property name="title">Library</property>
        </packing>
      </child>
      <child>
        <object class="GtkBox">
          <property name="visible">True</property>
          <property name="orientation">horizontal</property>
          <child>
            <object class="GtkButton">
              <property name="visible">True</property>
              <property name="label">Hi</property>
            </object>
          </child>
        </object>
        <packing>
          <property name="icon-name">computer-symbolic</property>
          <property name="title">Gameplay</property>
        </packing>
      </child>
      <child>
        <object class="GtkBox">
          <property name="visible">True</property>
          <property name="orientation">horizontal</property>
          <child>
            <object class="GtkButton">
              <property name="visible">True</property>
              <property name="label">Hi</property>
            </object>
          </child>
        </object>
        <packing>
          <property name="icon-name">input-gaming-symbolic</property>
          <property name="title">Controls</property>
        </packing>
      </child>
      <child>
        <object class="GtkBox">
          <property name="visible">True</property>
          <property name="orientation">vertical</property>
          <child>
            <object class="GtkTreeView" id="core-tree-view">
              <property name="visible">True</property>
              <property name="model">core-store</property>
              <child>
                <object class="GtkTreeViewColumn" id="core-name-column">
                  <property name="title">Core</property>
                  <child>
                    <object class="GtkCellRendererText" id="core-name-renderer"/>
                    <attributes>
                      <attribute name="text">0</attribute>
                    </attributes>
                  </child>
                </object>
              </child>
              <child>
                <object class="GtkTreeViewColumn" id="core-version-column">
                  <property name="title">Version</property>
                  <child>
                    <object class="GtkCellRendererText" id="core-version-renderer"/>
                    <attributes>
                      <attribute name="text">1</attribute>
                    </attributes>
                  </child>
                </object>
              </child>
            </object>
          </child>
        </object>
        <packing>
          <property name="icon-name">application-x-appliance-symbolic</property>
          <property name="title">Cores</property>
        </packing>
      </child>
    </object>
  </child>
</object>

